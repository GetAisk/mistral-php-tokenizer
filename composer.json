{
    "name": "aisk/mistral-tokenizer",
    "description": "PHP implementation of Mistral's tokenizers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aisk",
            "email": "info@getaisk.com",
            "homepage": "https://getaisk.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mistral\\Tokenizer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
